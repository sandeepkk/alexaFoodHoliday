<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
 
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<servlet-class>com.skk.HelloServlet</servlet-class>
  </servlet>
  
   <servlet>
  	<servlet-name>AlexaServlet</servlet-name>
  	<servlet-class>com.skk.AlexaServlet</servlet-class>
  </servlet>
  
<servlet-mapping>
	<servlet-name>AlexaServlet</servlet-name>
	<url-pattern>/SkkAlexaProject</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>HelloServlet</servlet-name>
	<url-pattern>/HelloAlexaProject</url-pattern>
</servlet-mapping>
  
</web-app>


